
procedure print_var(byte in value) is
;@j2c     printf("%d\n", value) ;
end procedure

var word e

for 10 using e loop

   case e of
      0 : print_var(0)
      1, 2 : print_var(12)
      
      3, 4 : block 
         print_var(34)
      end block
      
      5, 6 : block
      end block
   
      otherwise print_var(100+e)
      
   end case


end loop 
