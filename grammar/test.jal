
procedure print_var(byte in value) is
;@j2c     printf("%d\n", value) ;
end procedure

procedure increment(byte in out v) is
   v = v + 1
end procedure

procedure intermediate(byte in out u) is
   increment(u)
end procedure

function aap() return byte is
   return 1
end function

var byte a = 1

intermediate(a)

print_var(a)


a = aap() + 1
