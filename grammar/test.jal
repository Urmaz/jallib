
procedure print_var(byte in value) is
;@j2c     printf("%d\n", value) ;
end procedure

function plus_five(byte in invoer) return byte is   
   return invoer + 5
end function

var word e
 e = 5

forever loop   

   if (e == 10) then 
      e=20 
   elsif e==30 then 
      e=40 
   elsif e==50 then 
      exit loop
   else 
      e=plus_five(e) 
   end if

   print_var(e)

end loop
