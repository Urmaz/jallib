--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0xF {                -- oscillator selection bits
       RC_CLKOUT = 0xE                     -- 111x external rc oscillator, clkout function on ra6
       ECL_NOCLKOUT = 0xD                  -- ec oscillator (low power, <500 khz)
       ECL_CLKOUT = 0xC                    -- ec oscillator, clkout function on osc2 (low power, <500 khz)
       ECM_NOCLKOUT = 0xB                  -- ec oscillator (medium power, 500 khz-16 mhz)
       ECM_CLKOUT = 0xA                    -- ec oscillator, clkout function on osc2 (medium power, 500 khz-16 mhz)
       INTOSC_CLKOUT = 0x9                 -- internal oscillator block, clkout function on osc2
       INTOSC_NOCLKOUT = 0x8               -- internal oscillator block
       RC_NOCLKOUT = 0x7                   -- external rc oscillator
       ECH_NOCLKOUT = 0x5                  -- ec oscillator (high power, >16 mhz)
       ECH_CLKOUT = 0x4                    -- ec oscillator, clkout function on osc2 (high power, >16 mhz)
       HS_MEDIUM = 0x3                     -- hs oscillator (medium power 4-16 mhz)
       HS_HIGH = 0x2                       -- hs oscillator (high power > 16 mhz)
       XT = 0x1                            -- xt oscillator
       LP = 0x0                            -- lp oscillator
       }
pragma fuse_def PLLCFG:1 0x10 {            -- 4x pll enable
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x10                      -- enabled
       }
pragma fuse_def PRICLKEN:1 0x20 {          -- primary clock enable bit
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x20                      -- enabled
       }
pragma fuse_def FCMEN:1 0x40 {             -- fail-safe clock monitor enable bit
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x40                      -- enabled
       }
pragma fuse_def IESO:1 0x80 {              -- internal/external oscillator switchover bit
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x80                      -- enabled
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- power-up timer enable bit
       DISABLED = 0x1                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def BROWNOUT:2 0x6 {           -- brown-out reset enable bits
       ENABLED = 0x6                       -- brown-out reset enabled in hardware only (sboren is disabled)
       RUNONLY = 0x4                       -- brown-out reset enabled in hardware only and disabled in sleep mode (sboren is disabled)
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def VOLTAGE:2 0x18 {           -- brown out reset voltage bits
       V19 = 0x18                          -- vbor set to 1.90 v nominal
       V22 = 0x10                          -- vbor set to 2.20 v nominal
       V25 = 0x8                           -- vbor set to 2.50 v nominal
       V28 = 0x0                           -- vbor set to 2.85 v nominal
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x3 {                -- watchdog timer enable bits
       ENABLED = 0x3                       -- enabled
       CONTROL = 0x2                       -- wdt is controlled by swdten bit of the wdtcon register
       RUNONLY = 0x1                       -- wdt is disabled in sleep, otherwise enabled. swdten bit has no effect
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def WDTPS:3 0x3C {             -- watchdog timer postscale select bits
       P32K = 0x3C                         -- 1:32768
       P16K = 0x38                         -- 1:16384
       P8K = 0x34                          -- 1:8192
       P4K = 0x30                          -- 1:4096
       P2K = 0x2C                          -- 1:2048
       P1K = 0x28                          -- 1:1024
       P512  = 0x24                        -- 1:512
       P256  = 0x20                        -- 1:256
       P128  = 0x1C                        -- 1:128
       P64  = 0x18                         -- 1:64
       P32  = 0x14                         -- 1:32
       P16  = 0x10                         -- 1:16
       P8  = 0xC                           -- 1:8
       P4  = 0x8                           -- 1:4
       P2  = 0x4                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG3H (0x300005)
--
pragma fuse_def CCP2MUX:5 0x1 {            -- ccp2 mux bit
       pin_C1 = 0x1                        -- ccp2 input/output is multiplexed with rc1
       pin_B3 = 0x0                        -- ccp2 input/output is multiplexed with rb3
       }
pragma fuse_def PBADEN:5 0x2 {             -- portb a/d enable bit
       ANALOG = 0x2                        -- enabled
       DIGITAL = 0x0                       -- disabled
       }
pragma fuse_def CCP3MUX:5 0x4 {            -- p3a/ccp3 mux bit
       pin_B5 = 0x4                        -- p3a/ccp3 input/output is multiplexed with rb5
       pin_C6 = 0x0                        -- p3a/ccp3 input/output is mulitplexed with rc6
       }
pragma fuse_def HFOFST:5 0x8 {             -- hfintosc fast start-up
       ENABLED = 0x8                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def T3CMUX:5 0x10 {            -- timer3 clock input mux bit
       pin_C0 = 0x10                       -- t3cki is on rc0
       pin_B5 = 0x0                        -- t3cki is on rb5
