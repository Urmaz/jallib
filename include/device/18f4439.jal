--
-- Symbolic Fuse definitions
-- -------------------------
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0x7 {                -- oscillator
       HS_PLL = 0x6                        -- hs-pll enabled
       EC_NOCLKOUT = 0x5                   -- ec-osc2 as ra6
       EC_CLKOUT = 0x4                     -- ec-osc2 as clock out
       HS = 0x2                            -- hs
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- power up timer
       DISABLED = 0x1                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def BROWNOUT:2 0x2 {           -- brown out detect
       ENABLED = 0x2                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def VOLTAGE:2 0xC {            -- brown out voltage
       V25 = 0xC                           -- 2.5v
       V27 = 0x8                           -- 2.7v
       V42 = 0x4                           -- 4.2v
       V45 = 0x0                           -- 4.5v
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x1 {                -- watchdog timer
       ENABLED = 0x1                       -- enabled
       DISABLED = 0x0                      -- disabled-controlled by swdten bit
       }
pragma fuse_def WDTPS:3 0xE {              -- watchdog postscaler
       P128  = 0xE                         -- 1:128
       P64  = 0xC                          -- 1:64
       P32  = 0xA                          -- 1:32
       P16  = 0x8                          -- 1:16
       P8  = 0x6                           -- 1:8
       P4  = 0x4                           -- 1:4
       P2  = 0x2                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- RESERVED1 (0x300005)
--
--
-- CONFIG4L (0x300006)
--
pragma fuse_def STVR:6 0x1 {               -- stack overflow reset
       ENABLED = 0x1                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def LVP:6 0x4 {                -- low voltage program
       ENABLED = 0x4                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def DEBUG:6 0x80 {             -- background debug
       DISABLED = 0x80                     -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG5L (0x300008)
--
pragma fuse_def CP0:8 0x1 {                -- code protect 00200-01fff
       DISABLED = 0x1                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def CP1:8 0x2 {                -- code protect 02000-03fff
       DISABLED = 0x2                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def CP2:8 0x4 {                -- code protect 04000-05fff
       DISABLED = 0x4                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG5H (0x300009)
