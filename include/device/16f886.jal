--
-- Symbolic Fuse definitions
-- -------------------------
--
-- CONFIG (0x2007)
--
pragma fuse_def OSC:0 0x7 {                -- oscillator
       RC_CLKOUT = 0x7                     -- external rc clockout
       RC_NOCLKOUT = 0x6                   -- external rc no clock
       INTOSC_CLKOUT = 0x5                 -- internal rc clockout
       INTOSC_NOCLKOUT = 0x4               -- internal rc no clock
       EC_NOCLKOUT = 0x3                   -- ec
       HS = 0x2                            -- hs
       XT = 0x1                            -- xt
       LP = 0x0                            -- lp
       }
pragma fuse_def WDT:0 0x8 {                -- watchdog timer
       ENABLED = 0x8                       -- on
       DISABLED = 0x0                      -- off
       }
pragma fuse_def PWRTE:0 0x10 {             -- power up timer
       DISABLED = 0x10                     -- off
       ENABLED = 0x0                       -- on
       }
pragma fuse_def MCLR:0 0x20 {              -- master clear enable
       INTERNAL = 0x0                      -- re3 is digital input
       EXTERNAL = 0x20                     -- /mclr is external
       }
pragma fuse_def CP:0 0x40 {                -- code protect
       DISABLED = 0x40                     -- off
       ENABLED = 0x0                       -- on
       }
pragma fuse_def CPD:0 0x80 {               -- data ee read protect
       DISABLED = 0x80                     -- off
       ENABLED = 0x0                       -- on
       }
pragma fuse_def BROWNOUT:0 0x300 {         -- brown out detect
       DISABLED = 0x0                      -- bod and sboren disabled
       CONTROL = 0x100                     -- sboren controls bor function
       RUNONLY = 0x200                     -- bod enabled in run, disabled in sleep, sboren disabled
       ENABLED = 0x300                     -- bod enabled, sboren disabled
       }
pragma fuse_def IESO:0 0x400 {             -- internal external switch over mode
       ENABLED = 0x400                     -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def FCMEN:0 0x800 {            -- monitor clock fail-safe
       ENABLED = 0x800                     -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def LVP:0 0x1000 {             -- low voltage program
       ENABLED = 0x1000                    -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def DEBUG:0 0x2000 {           -- background debug
       DISABLED = 0x2000                   -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG2 (0x2008)
--
pragma fuse_def VOLTAGE:1 0x100 {          -- brown out reset sel bit
       V21 = 0x0                           -- brown out at 2.1v
       V40 = 0x100                         -- brown out at 4.0v
       }
pragma fuse_def WRT:1 0x600 {              -- self write enable
       R0000_0FFF = 0x0                    -- 0000-0fff prot
       R0000_07FF = 0x200                  -- 0000-07ff prot
       R0000_00FF = 0x400                  -- 0000-00ff prot
       NO_PROTECTION = 0x600               -- no protection
       }
