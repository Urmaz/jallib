pragma fuse_def RETEN:12 0x1 {             -- retention voltage regulator control enable
       DISABLED = 0x1                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def VBTBOR:12 0x4 {            -- vbat bor enable
       ENABLED = 0x4                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def BROWNOUT:12 0x8 {          -- deep sleep bor enable
       ENABLED = 0x8                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def DSBITEN:12 0x10 {          -- dsen bit enable bit
       ENABLED = 0x10                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
