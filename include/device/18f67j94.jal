--
-- CONFIG1L (0x1FFF0)
--
pragma fuse_def STVR:0 0x20 {              -- stack overflow/underflow reset
       ENABLED = 0x20                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def XINST:0 0x40 {             -- extended instruction set
       ENABLED = 0x40                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def DEBUG:0 0x80 {             -- background debug
       DISABLED = 0x80                     -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG1H (0x1FFF1)
--
pragma fuse_def BROWNOUT:1 0x1 {           -- brown-out reset enable
       ENABLED = 0x1                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def VOLTAGE:1 0x2 {            -- brown-out reset voltage
       V18 = 0x2                           -- 1.8v
       V20 = 0x0                           -- 2.0v
       }
pragma fuse_def CP0:1 0x4 {                -- code protect
       DISABLED = 0x4                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def SIGN:1 0x8 {               -- config word signature bit
       NOT_CONDUCATED = 0x8                -- bulk erase of memory not conducted
       AREA_COMPLETE = 0x0                 -- bulk erase of memory area complete
       }
--
-- CONFIG2L (0x1FFF2)
--
pragma fuse_def OSC:2 0x7 {                -- oscillator
       RC_FAST_POST = 0x7                  -- fast rc oscillator with postscaler (frcdiv)
       RC_FAST_DIV16 = 0x6                 -- fast rc oscillator divided by 16 (500 khz)
       LP = 0x5                            -- low-power rc oscillator (lprc)
       SOCS = 0x4                          -- secondary oscillator (sosc)
       PRIM_PLL = 0x3                      -- primary oscillator with pll module (xtpll,hspll,ecpll)
       PRIM = 0x2                          -- primary oscillator (xt, hs, ec)
       RC_FAST_PLL = 0x1                   -- fast rc oscillator with pll module (frcpll)
       RC_FAST = 0x0                       -- fast rc oscillator (frc)
       }
pragma fuse_def SOSCSEL:2 0x8 {            -- t1osc/sosc power selection bits
       T1OSC = 0x8                         -- low power t1osc/sosc circuit selected
       SCLKI = 0x0                         -- digital (sclki) mode
       }
pragma fuse_def CLKOEN:2 0x20 {            -- clock out enable bit
       ENABLED = 0x20                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def IESO:2 0x80 {              -- internal external oscillator switch over mode
       ENABLED = 0x80                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
--
-- CONFIG2H (0x1FFF3)
--
pragma fuse_def PLLDIV:3 0xF {             -- pll frequency multiplier select bits
       P0 = 0xF                            -- no pll used - pllgo bit not available to user
       X8 = 0xE                            -- 8x pll selected
       X6 = 0xD                            -- 6x pll selected
       X4 = 0xC                            -- 4x pll selected
       P12 = 0x7                           -- 96 mhz pll selected; oscillator divided by 12 (48 mhz input)
       P10 = 0x6                           -- 96 mhz pll selected; oscillator divided by 10 (40 mhz input)
       P6 = 0x5                            -- 96 mhz pll selected; oscillator divided by 6 (24 mhz input)
       P5 = 0x4                            -- 96 mhz pll selected; oscillator divided by 5 (20 mhz input)
       P4 = 0x3                            -- 96 mhz pll selected; oscillator divided by 4 (16 mhz input)
       P3 = 0x2                            -- 96 mhz pll selected; oscillator divided by 3 (12 mhz input)
       P2 = 0x1                            -- 96 mhz pll selected; oscillator divided by 2 (8 mhz input)
       P1 = 0x0                            -- 96 mhz pll selected; no divide - oscillator used directly (4 mhz input)
       }
--
-- CONFIG3L (0x1FFF4)
--
pragma fuse_def POSCMD:4 0x3 {             -- primary oscillator select
       DISABLED = 0x3                      -- primary oscillator disabled
       HS = 0x2                            -- hs oscillator mode selected(10 mhz - 40 mhz)
       MS = 0x1                            -- ms oscillator mode selected(3.5 mhz -10 mhz)
       EC = 0x0                            -- external clock mode selected
       }
pragma fuse_def FCMEN:4 0x30 {             -- clock switching and monitor selection configuration bits
       DISABLED = 0x20                     -- clock switching is disabled, fail safe clock monitor is disabled
       ENABLED = 0x10                      -- clock switching is enabled, fail safe clock monitor is disabled
       }
