--
-- Special (device specific) constants and procedures
--
const      ADC_GROUP          = ADC_V12
const byte ADC_NTOTAL_CHANNEL = 16
const byte ADC_ADCS_BITCOUNT  = 0
const byte ADC_MAX_RESOLUTION = 12
--
const PPS_GROUP  = PPS_0        -- no Peripheral Pin Selection
--
const word USB_BDT_ADDRESS    = 0x0D00
--
-- - - - - - - - - - - - - - - - - - - - - - - - - - -
-- Change analog I/O pins into digital I/O pins.
procedure analog_off() is
   pragma inline
   ANCON1 = 0b0000_0000        -- digital I/O
   ANCON2 = 0b0000_0000        -- digital I/O
   ANCON3 = 0b0000_0000        -- digital I/O
end procedure
--
-- - - - - - - - - - - - - - - - - - - - - - - - - - -
-- Disable comparator module
procedure comparator_off() is
   pragma inline
   CM1CON = 0b0000_0000        -- disable comparator
   CM2CON = 0b0000_0000        -- digital I/O
   CM3CON = 0b0000_0000
end procedure
--
-- - - - - - - - - - - - - - - - - - - - - - - - - - -
-- Switch analog ports to digital mode (if analog module present).
procedure enable_digital_io() is
   pragma inline
   analog_off()
   comparator_off()
end procedure
