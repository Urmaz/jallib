-- CONFIG4H (0x1FFF7)
--
pragma fuse_def WPDIS:7 0x1 {              -- segment write protection disable
       DISABLED = 0x1                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def WPEND:7 0x2 {              -- segment write protection end page select
       PWPFP_END = 0x2                     -- write protect from wpfp to the last page of memory
       P0_WPFP = 0x0                       -- write protect from page 0 to wpfp
       }
pragma fuse_def WPCFG:7 0x4 {              -- write protect configuration page select
       DISABLED = 0x4                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG5L (0x1FFF8)
--
pragma fuse_def T5GSEL:8 0x1 {             -- tmr5 gate select bit
       T5G = 0x1                           -- tmr5 gate is driven by the t5g input
       T3G = 0x0                           -- tmr5 gate is driven by the t3g input
       }
pragma fuse_def CINASEL:8 0x2 {            -- cxina gate select bit
       DEFAULT = 0x2                       -- c1ina and c3ina are on their default pin locations
       MAPPED = 0x0                        -- c1ina and c3ina are all re-mapped to pin ra5
       }
--
-- CONFIG5H (0x1FFF9)
--
pragma fuse_def IOL1WAY:9 0x1 {            -- iolock one-way set enable bit
       ENABLED = 0x1                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def LS48MHZ:9 0x2 {            -- usb low speed clock select bit
       P2 = 0x2                            -- divide-by-2 (system clock must be 12 mhz)
       P1 = 0x0                            -- divide-by-1 (system clock must be 6 mhz)
       }
pragma fuse_def MSSPMSK2:9 0x4 {           -- mssp2 7-bit address masking mode enable bit
       B7 = 0x4                            -- 7 bit address masking mode
       B5 = 0x0                            -- 5 bit address masking mode
       }
pragma fuse_def MSSPMSK1:9 0x8 {           -- mssp1 7-bit address masking mode enable bit
       B7 = 0x8                            -- 7 bit address masking mode
       B5 = 0x0                            -- 5 bit address masking mode
       }
--
-- CONFIG6L (0x1FFFA)
--
pragma fuse_def WDTWIN:10 0x3 {            -- watch dog timer window
       P25 = 0x3                           -- watch dog timer window width is 25 percent
       P37 = 0x2                           -- watch dog timer window width is 37.5 percent
       P50 = 0x1                           -- watch dog timer window width is 50 percent
       P75 = 0x0                           -- watch dog timer window width is 75 percent
       }
pragma fuse_def WDTCLK:10 0xC {            -- watch dog timer clock source
       FRC = 0xC                           -- use frc when windis = 0, system clock is not intosc/lprc and device is not in sleep; otherwise, use intosc/lprc
       INTOSC = 0x8                        -- always use intosc/lprc
       SOCS = 0x4                          -- always use sosc
       FOSC = 0x0                          -- use fosc/4 when system clock is not intosc/lprc and device is not in sleep; otherwise, use intosc/lprc
       }
pragma fuse_def WDTPS:10 0xF0 {            -- watchdog timer postscale
       P32K = 0xF0                         -- 1:32768
       P16K = 0xE0                         -- 1:16384
       P8K = 0xD0                          -- 1:8192
       P4K = 0xC0                          -- 1:4096
       P2K = 0xB0                          -- 1:2048
       P1K = 0xA0                          -- 1:1024
       P512  = 0x90                        -- 1:512
       P256  = 0x80                        -- 1:256
       P128  = 0x70                        -- 1:128
       P64  = 0x60                         -- 1:64
       P32  = 0x50                         -- 1:32
       P16  = 0x40                         -- 1:16
       P8  = 0x30                          -- 1:8
