       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG6H (0x1FFFB)
--
pragma fuse_def WDT:11 0x3 {               -- watchdog timer enable
       HARDWARE = 0x3                      -- wdt enabled in hardware; swdten bit disabled
       ENABLED = 0x2                       -- enabled
       RUNONLY = 0x1                       -- wdt enabled only while device is active and disabled in sleep mode; swdten bit disabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def WINDIS:11 0x4 {            -- windowed watchdog timer disable
       DISABLED = 0x4                      -- standard wdt selected; windowed wdt disabled
       ENABLED = 0x0                       -- windowed wdt enabled
       }
