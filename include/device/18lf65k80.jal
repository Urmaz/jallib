--
-- CONFIG1L (0x300000)
--
pragma fuse_def RETEN:0 0x1 {              -- vreg sleep enable bit
       ENABLED = 0x0                       -- enabled
       DISABLED = 0x1                      -- disabled
       }
pragma fuse_def INTOSCSEL:0 0x4 {          -- lf-intosc low-power enable bit
       HS_CP = 0x4                         -- high security code protection enable
       LF_INTOSC = 0x0                     -- lf-intosc in low-power mode during sleep
       }
pragma fuse_def SOSCSEL:0 0x18 {           -- sosc power selection and mode configuration bits
       HP_SOSC = 0x18                      -- high power sosc circuit selected
       LP_SOSC = 0x8                       -- low power sosc circuit selected
       SCLKI = 0x10                        -- digital (sclki) mode
       }
pragma fuse_def XINST:0 0x40 {             -- extended instruction set
       ENABLED = 0x40                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0xF {                -- oscillator
       RC_NOCLKOUT = 0x7                   -- external rc oscillator
       RC_CLKOUT = 0x6                     -- external rc oscillator, clkout function on osc2
       ECL_NOCLKOUT = 0xD                  -- ec oscillator (low power, dc - 160 khz)
       ECL_CLKOUT = 0xC                    -- ec oscillator, clkout function on osc2 (low power, dc - 160 khz)
       ECM_NOCLKOUT = 0xB                  -- ec oscillator (medium power, 160 khz - 4 mhz)
       ECM_CLKOUT = 0xA                    -- ec oscillator, clkout function on osc2 (medium power, 160 khz - 4 mhz)
       INTOSC_CLKOUT = 0x9                 -- internal rc oscillator, clkout function on osc2
       INTOSC_NOCLKOUT = 0x8               -- internal rc oscillator
       ECH_NOCLKOUT = 0x5                  -- ec oscillator (high power, 16 mhz - 64 mhz)
       ECH_CLKOUT = 0x4                    -- ec oscillator, clkout function on osc2 (high power, 16 mhz - 64 mhz)
       HS_MEDIUM = 0x3                     -- hs oscillator (medium power, 4 mhz - 16 mhz)
       HS_HIGH = 0x2                       -- hs oscillator (high power, 16 mhz - 25 mhz)
       XT = 0x1                            -- xt oscillator
       LP = 0x0                            -- lp oscillator
       }
pragma fuse_def PLLCFG:1 0x10 {            -- pll x4 enable bit
       ENABLED = 0x10                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def FCMEN:1 0x40 {             -- fail-safe clock monitor
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x40                      -- enabled
       }
pragma fuse_def IESO:1 0x80 {              -- internal external oscillator switch over mode
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x80                      -- enabled
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- power up timer
       DISABLED = 0x1                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def BROWNOUT:2 0x6 {           -- brown out detect
       ENABLED = 0x6                       -- enabled in hardware, sboren disabled
       RUNONLY = 0x4                       -- enabled while active, disabled in sleep, sboren disabled
       CONTROL = 0x2                       -- controlled with sboren bit
       DISABLED = 0x0                      -- disabled in hardware, sboren disabled
       }
pragma fuse_def VOLTAGE:2 0x18 {           -- brown-out reset voltage bits
       V18 = 0x18                          -- 1.8v
       V20 = 0x10                          -- 2.0v
       V27 = 0x8                           -- 2.7v
       V30 = 0x0                           -- 3.0v
       }
pragma fuse_def BORPWR:2 0x60 {            -- bormv power level
       ZPBORMV = 0x60                      -- zpbormv instead of bormv is selected
       HP_BORMV = 0x40                     -- bormv set to high power level
       MP_BORMV = 0x20                     -- bormv set to medium power level
       LP_BORMV = 0x0                      -- bormv set to low power level
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x3 {                -- watchdog timer
       ENABLED = 0x3                       -- wdt enabled in hardware; swdten bit disabled
       CONTROL = 0x2                       -- wdt controlled by swdten bit setting
       RUNONLY = 0x1                       -- wdt enabled only while device is active and disabled in sleep mode; swdten bit disabled
       DISABLED = 0x0                      -- wdt disabled in hardware; swdten bit disabled
       }
pragma fuse_def WDTPS:3 0x7C {             -- watchdog postscaler
       P1M = 0x7C                          -- 1:1048576
       P512K = 0x4C                        -- 1:524288
       P256K = 0x48                        -- 1:262144
       P128K = 0x44                        -- 1:131072
       P64K = 0x40                         -- 1:65536
       P32K = 0x3C                         -- 1:32768
       P16K = 0x38                         -- 1:16384
       P8K = 0x34                          -- 1:8192
       P4K = 0x30                          -- 1:4096
       P2K = 0x2C                          -- 1:2048
       P1K = 0x28                          -- 1:1024
       P512  = 0x24                        -- 1:512
       P256  = 0x20                        -- 1:256
       P128  = 0x1C                        -- 1:128
       P64  = 0x18                         -- 1:64
       P32  = 0x14                         -- 1:32
       P16  = 0x10                         -- 1:16
       P8  = 0xC                           -- 1:8
       P4  = 0x8                           -- 1:4
       P2  = 0x4                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
