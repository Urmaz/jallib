--
-- Symbolic Fuse definitions
-- -------------------------
--
-- CONFIG1L (0xFFF8)
--
pragma fuse_def WDT:0 0x1 {                -- watchdog timer
       ENABLED = 0x1                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def STVR:0 0x20 {              -- stack overflow/underflow reset 
       ENABLED = 0x20                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def XINST:0 0x40 {             -- extended instruction set
       ENABLED = 0x40                      -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def DEBUG:0 0x80 {             -- background debug
       DISABLED = 0x80                     -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG1H (0xFFF9)
--
pragma fuse_def CP0:1 0x4 {                -- code protect
       DISABLED = 0x4                      -- disabled
       ENABLED = 0x0                       -- enabled
       }
pragma fuse_def SIGN:1 0x8 {               -- config word signature bit
       NOT_CONDUCATED = 0x8                -- bulk erase of memory not conducated
       AREA_COMPLETE = 0x0                 -- bulk erase of memory area complete
       }
--
-- CONFIG2L (0xFFFA)
--
pragma fuse_def OSC:2 0x7 {                -- oscillator
       EC_CLKOUT_PLL = 0x7                 -- ec+pll (clko-ra6)
       EC_CLKOUT = 0x6                     -- ec (clko-ra6)
       HS_PLL = 0x5                        -- hs+pll
       HS = 0x4                            -- hs
       INTOSC_CLKOUT_PLL = 0x3             -- intoscpllo (clko-ra6)
       INTOSC_NOCLKOUT_PLL = 0x2           -- intoscpll
       INTOSC_CLKOUT = 0x1                 -- intosco (clko-ra6)
       }
pragma fuse_def T1DIG:2 0x8 {              -- t1oscen enforcement
       ENABLED = 0x8                       -- enabled
       DISABLED = 0x0                      -- disabled
       }
pragma fuse_def LPT1OSC:2 0x10 {           -- low-power timer1 oscillator
       HIGH_POWER = 0x10                   -- disabled
       LOW_POWER = 0x0                     -- enabled
       }
pragma fuse_def FCMEN:2 0x40 {             -- fail-safe clock monitor
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x40                      -- enabled
       }
pragma fuse_def IESO:2 0x80 {              -- internal external oscillator switch over mode
       DISABLED = 0x0                      -- disabled
       ENABLED = 0x80                      -- enabled
       }
--
-- CONFIG2H (0xFFFB)
--
pragma fuse_def WDTPS:3 0xF {              -- watchdog postscaler
       P32K = 0xF                          -- 1:32768
       P16K = 0xE                          -- 1:16384
       P8K = 0xD                           -- 1:8192
       P4K = 0xC                           -- 1:4096
       P2K = 0xB                           -- 1:2048
       P1K = 0xA                           -- 1:1024
       P512  = 0x9                         -- 1:512
       P256  = 0x8                         -- 1:256
       P128  = 0x7                         -- 1:128
       P64  = 0x6                          -- 1:64
       P32  = 0x5                          -- 1:32
       P16  = 0x4                          -- 1:16
       P8  = 0x3                           -- 1:8
       P4  = 0x2                           -- 1:4
       P2  = 0x1                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG3L (0xFFFC)
