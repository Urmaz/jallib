--
-- CONFIG (0x2007)
--
pragma fuse_def OSC:0 0x7 {                -- oscillator
       RC_CLKOUT = 0x7                     -- external rc clockout
       RC_NOCLKOUT = 0x6                   -- external rc no clock
       INTOSC_CLKOUT = 0x5                 -- internal rc clockout
       INTOSC_NOCLKOUT = 0x4               -- internal rc no clock
       EC_NOCLKOUT = 0x3                   -- ec
       HS = 0x2                            -- hs
       XT = 0x1                            -- xt
       LP = 0x0                            -- lp
       }
pragma fuse_def WDT:0 0x8 {                -- watchdog timer
       ENABLED = 0x8                       -- on
       DISABLED = 0x0                      -- off
       }
pragma fuse_def PWRTE:0 0x10 {             -- power up timer
       DISABLED = 0x10                     -- off
       ENABLED = 0x0                       -- on
       }
pragma fuse_def MCLR:0 0x20 {              -- master clear enable
       INTERNAL = 0x0                      -- re3 is digital input
       EXTERNAL = 0x20                     -- /mclr is external
       }
pragma fuse_def CP:0 0x40 {                -- code protect
       DISABLED = 0x40                     -- off
       ENABLED = 0x0                       -- on
       }
pragma fuse_def BROWNOUT:0 0x300 {         -- brown out reset enable
       DISABLED = 0x0                      -- bor disabled
       RUNONLY = 0x200                     -- bor enabled in run, disabled in sleep
       ENABLED = 0x300                     -- bor enabled
       }
pragma fuse_def VOLTAGE:0 0x400 {          -- brown-out reset voltage selection
       V19 = 0x400                         -- 1.9v
       V25 = 0x0                           -- 2.5v
       }
pragma fuse_def WDTCS:0 0x800 {            -- watchdog timer clock select
       STANDARD = 0x800                    -- standard watchdog timer is selected
       LOW_POWER = 0x0                     -- low power watchdog timer is selected
       }
pragma fuse_def PLLEN:0 0x1000 {           -- intosc pllen enable bit
       F16MHZ = 0x1000                     -- 16mhz
       F500KHZ = 0x0                       -- 500khz
       }
pragma fuse_def DEBUG:0 0x2000 {           -- background debug
       DISABLED = 0x2000                   -- disabled
       ENABLED = 0x0                       -- enabled
       }
--
-- CONFIG2 (0x2008)
--
pragma fuse_def VCAPEN:1 0x30 {            -- voltage regulator capacitor enable bits
       DISABLED = 0x30                     -- all vcap pin functions are disabled
       pin_A6 = 0x20                       -- vcap functionality is enabled on ra6
       pin_A5 = 0x10                       -- vcap functionality is enabled on ra5
       pin_A0 = 0x0                        -- vcap functionality is enabled on ra0
       }
