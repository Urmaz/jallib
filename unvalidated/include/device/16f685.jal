var volatile bit   ANSEL_ANS7           at ANSEL : 7
var volatile bit   ANSEL_ANS6           at ANSEL : 6
var volatile bit   ANSEL_ANS5           at ANSEL : 5
var volatile bit   ANSEL_ANS4           at ANSEL : 4
var volatile bit   ANSEL_ANS3           at ANSEL : 3
var volatile bit   ANSEL_ANS2           at ANSEL : 2
var volatile bit   ANSEL_ANS1           at ANSEL : 1
var volatile bit   ANSEL_ANS0           at ANSEL : 0
-- ------------------------------------------------
var volatile byte  ANSELH               at { 0x11F }
var volatile bit*4 ANSELH_ANSELH        at ANSELH : 0
-- ------------------------------------------------
var volatile byte  EECON1               at { 0x18C }
var volatile bit   EECON1_EEPGD         at EECON1 : 7
var volatile bit   EECON1_WRERR         at EECON1 : 3
var volatile bit   EECON1_WREN          at EECON1 : 2
var volatile bit   EECON1_WR            at EECON1 : 1
var volatile bit   EECON1_RD            at EECON1 : 0
-- ------------------------------------------------
var volatile byte  EECON2               at { 0x18D }
-- ------------------------------------------------
var volatile byte  PSTRCON              at { 0x19D }
var volatile bit   PSTRCON_STRSYNC      at PSTRCON : 4
var volatile bit   PSTRCON_STRD         at PSTRCON : 3
var volatile bit   PSTRCON_STRC         at PSTRCON : 2
var volatile bit   PSTRCON_STRB         at PSTRCON : 1
var volatile bit   PSTRCON_STRA         at PSTRCON : 0
-- ------------------------------------------------
var volatile byte  SRCON                at { 0x19E }
var volatile bit*2 SRCON_SR             at SRCON : 6
var volatile bit   SRCON_C1SEN          at SRCON : 5
var volatile bit   SRCON_C2REN          at SRCON : 4
var volatile bit   SRCON_PULSS          at SRCON : 3
var volatile bit   SRCON_PULSR          at SRCON : 2
--
-- ===================================================
--
-- Special device dependent procedures
--
-- ---------------------------------------------------
-- Change analog I/O pins into digital I/O pins.
--
procedure analog_off() is
   pragma inline
   ANSEL  = 0b0000_0000       -- all digital
   ANSELH = 0b0000_0000       -- all digital
end procedure
--
-- ---------------------------------------------------
-- Disable ADC module (ADC_group ADC_V0)
--
procedure adc_off() is
   pragma inline
   ADCON0 = 0b0000_0000         -- disable ADC
   ADCON1 = 0b0000_0000
end procedure
--
-- ---------------------------------------------------
-- Switch analog ports to digital mode (if analog module present).
--
procedure enable_digital_io() is
   pragma inline
   analog_off()
   adc_off()
end procedure
